{
  "name": "rnc",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:staging": "ENVFILE=.env.staging react-native run-android --variant=stagingDebug --appIdSuffix=stg",
    "android:staging-release": "react-native run-android --variant=stagingRelease --appIdSuffix=stg",
    "android:dev": "ENVFILE=.env.development react-native run-android --variant=developmentDebug --appIdSuffix=dev",
    "android:dev-release": "react-native run-android --variant=developmentRelease --appIdSuffix=dev",
    "android:prod": "react-native run-android --variant=productionDebug --appIdSuffix=stg",
    "android:prod-release": "react-native run-android --variant=productionRelease --appIdSuffix=stg",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.68.1",
    "react-native-config": "^1.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/runtime": "^7.17.9",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^28.0.3",
    "eslint": "^8.14.0",
    "jest": "^28.0.3",
    "metro-react-native-babel-preset": "^0.70.2",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
